@page "/"
@using Microsoft.AspNetCore.Authorization
@using Microsoft.Graph
@using EmployeeDrawings.Data;
@inject Microsoft.Graph.GraphServiceClient GraphServiceClient
@inject IJSRuntime JsRuntime

<!-- attribute [Authorize] -->

<PageTitle>Employee Drawings</PageTitle>

<h1>Employee Drawings</h1>

<InputFile OnChange="@FileSelectionHandler" />

<div>
<label for="numWinners" id="numWinnersLabel">Number of Winners: </label>
<input type="number" id="numWinners" @onchange="NumWinnersChangeHandler" required />

<label for="numAlternates" id="numAlternates" style="padding-left: 5vw;">Number of Alternates: </label>
<input type="number" id="numAlternates" @onchange="NumAlternatesChangeHandler" required />

<label style="padding-left: 5vw;"></label>
<button @onclick="@ExecuteDrawing">Generate Results of Drawing</button>
</div>

<br />

@if (users != null) {
    <ul class="nav nav-pills nav-fill">
        @if (currentDisplayContext.Equals(DisplayContext.Selected)) {
            <li class="nav-item">
                <a class="nav-link active" aria-current="page" href="#" @onclick="@(e => UpdateDisplayContext(DisplayContext.Selected))">Selected : @numSelected </a>
            </li>
        } else {
            <li class="nav-item">
                <a class="nav-link" aria-current="page" href="#" @onclick="@(e => UpdateDisplayContext(DisplayContext.Selected))">Selected : @numSelected </a>
            </li>
        }

        @if (currentDisplayContext.Equals(DisplayContext.Winners)) {
            <li class="nav-item">
                <a class="nav-link active" href="#" @onclick="@(e => UpdateDisplayContext(DisplayContext.Winners))">Winners : @(winners?.Count() ?? 0) </a>
            </li>
        } else {
            <li class="nav-item">
                <a class="nav-link" href="#" @onclick="@(e => UpdateDisplayContext(DisplayContext.Winners))">Winners : @(winners?.Count() ?? 0) </a>
            </li>
        }

        @if (currentDisplayContext.Equals(DisplayContext.Alternates)) {
            <li class="nav-item">
                <a class="nav-link active" href="#" @onclick="@(e => UpdateDisplayContext(DisplayContext.Alternates))">Alternates : @(alternates?.Count() ?? 0) </a>
            </li>
        } else {
            <li class="nav-item">
                <a class="nav-link" href="#" @onclick="@(e => UpdateDisplayContext(DisplayContext.Alternates))">Alternates : @(alternates?.Count() ?? 0) </a>
            </li>
        }

        <button type="button" @onclick="DownloadPdf">Download Lists</button>
    </ul>

    <br />

}

@if (displayList != null) {
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Department</th>
                <th>Name</th>
                <th>EE#</th>
                <th>Position</th>
                <th>Supervisor</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in displayList) {
                <tr>
                    <td> @user["Department"] </td>
                    <td> @user["Surname"], @user["GivenName"] @user["MiddleInitial"] </td>
                    <td> @user["EIN"] </td>
                    <td> @user["JobTitle"] </td>
                    <td> @user["Supervisor"] </td>
                </tr>
            }
        </tbody>
    </table>
}

