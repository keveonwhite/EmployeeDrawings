    private void NumWinnersChangeHandler(ChangeEventArgs e) {
        if (e.Value != null) { numWinners = Int32.Parse(e.Value.ToString()!.Equals("") ? "0" : e.Value.ToString()!); }
        isGenerated = false;
    }

    private void NumAlternatesChangeHandler(ChangeEventArgs e) {
        if (e.Value != null) { numAlternates = Int32.Parse(e.Value.ToString()!.Equals("") ? "0" : e.Value.ToString()!); }
        isGenerated = false;
    }

    private async void ExecuteDrawing() {
        if (isGenerated) { 
            await alert("Cannot generate new list without changing file, number of winners, or number of alternates");
            return;
        }

        isGenerated = true;
        try {
            await ReadFile();

            if (numAlternates == null) { numAlternates = 0; }

            if (users == null) { await alert("File not read"); }
            else if (numWinners == null) {
                await alert("A number of winners must be selected before generating a result");
                return;
            }
            else if (numWinners <= 0) {
                await alert("Number of winners must be greater than 0");
                return;
            }
            else if (numWinners > users.Count()) {
                await alert("The number of winners must be less than or equal to the number of selected employees");
                return;
            }
            else if (numAlternates < 0) {
                await alert("The number of alternates must be positive");
                return;
            }
            else if (numWinners + numAlternates > users.Count()) {
                await alert("The number of winners and alternates, combined, must be less than the number of selected employees");
                return;
            }
            else {
                List<Dictionary<string, string>> tmp = users.ToList<Dictionary<string, string>>();
                int idx = r.Next(tmp.Count());
                winners = new List<Dictionary<string, string>>();
                for (int i = 0; i < numWinners; i++) {
                    winners = winners.Append(tmp[idx]);
                    tmp.RemoveAt(idx);
                    idx = r.Next(tmp.Count());
                }

                alternates = new List<Dictionary<string, string>>();
                for (int i = 0; i < numAlternates; i++) {
                    alternates = alternates.Append(tmp[idx]);
                    tmp.RemoveAt(idx);
                    idx = r.Next(tmp.Count());
                }

                winners = OrderList(winners);
                alternates = OrderList(alternates);

                UpdateDisplayContext(currentDisplayContext);
                StateHasChanged();
            }
        } catch (Exception e) {
            Console.WriteLine(e);
        }
    }

    private async void FileSelectionHandler(InputFileChangeEventArgs e) {
        if (e.File.Name.EndsWith(".csv")) {
            file = e.File;
            isGenerated = false;
        } else {
            await alert("Only .csv accepted as file type");
        }
    }

    private IEnumerable<Dictionary<string, string>> OrderList(IEnumerable<Dictionary<string, string>> l) {
        return l.ToList<Dictionary<string, string>>(
                ).OrderBy(
                   d => d["Department"]
                ).ThenBy(
                   d => d["Surname"]
                ).ThenBy(
                   d => d["GivenName"]
                ).ThenBy(
                   d => d["EIN"]
                );
    }

    private async Task ReadFile() {
        try {
            if (file != null) {
                var stream = file.OpenReadStream();
                var buffer = new byte[file.Size];
                await stream.ReadAsync(buffer, 0, (int)file.Size);
                fileContent = System.Text.Encoding.UTF8.GetString(buffer);

                string[] lines = fileContent.Split("\n", StringSplitOptions.TrimEntries);
                lines[0] = lines[0].Remove(0,1);    // remove the first character from first line (which is not trimmable)
                users = new List<Dictionary<string, string>>();

                foreach (string line in lines) {
                    if (line.Count() == 0) { continue; }
                    string ein = line.Split(",", StringSplitOptions.TrimEntries)[0];

                    if (ein.Count() == 0) { continue; }

                    Dictionary<string, string>? userInfo;
                    if (userInfoDictionary.TryGetValue(ein, out userInfo)) {
                        if (users.Contains(userInfo)) { continue; }
                        users = users.Append(userInfo);
                    }
                    else { Console.WriteLine("Couldn't find employee number " + ein); }
                }

                users = OrderList(users);
                numSelected = users.Count();

                UpdateDisplayContext(currentDisplayContext);
                StateHasChanged();

                fileContent = null;
            }
            else {
                await alert("Ensure file type is .csv");
            }
        } catch (Exception ex) {
            Console.WriteLine(ex);
        }
    }

    private void UpdateDisplayContext(DisplayContext displayContext) {
        switch (displayContext) {
            case DisplayContext.Selected: displayList = users; break;
            case DisplayContext.Winners: displayList = winners; break;
            case DisplayContext.Alternates: displayList = alternates; break;
            default: break;
        }

        currentDisplayContext = displayContext;
    }

